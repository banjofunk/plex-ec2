{
  "name": "plex-ec2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "set-password": "f(){ aws ssm put-parameter --name /plex-ec2/password --type SecureString --overwrite --value $1 | cat; };f",
    "set-username": "f(){ aws ssm put-parameter --name /plex-ec2/username --type String --overwrite --value $1 | cat; };f",
    "upload-templates": "node ./templates/uploadTemplates.js",
    "plex-create": "(cd services/ec2; serverless invoke -f createPlexEc2Instance)",
    "plex-delete": "(cd services/ec2; serverless invoke -f deletePlexEc2Instance)",
    "plex-start": "(cd services/ec2; serverless invoke -f startEc2Instance)",
    "plex-stop": "(cd services/ec2; serverless invoke -f stopEc2Instance)",
    "plex-up": "(cd services/plex; serverless invoke -f invoke_plex_server_up)",
    "plex-down": "(cd services/plex; serverless invoke -f invoke_plex_server_down)",
    "deploy": "sh ./deploy.sh",
    "undeploy": "sh ./undeploy.sh",
    "recreate": "sh ./recreate.sh"
  },
  "dependencies": {
    "serverless-plugin-scripts": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-fivetalent": "0.0.24",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "prettier": "^1.19.1",
    "serverless-pseudo-parameters": "^2.5.0"
  }
}
